<p id="notice"><%= notice %></p>

<h1>Cans</h1>

<table>
  <thead>
    <tr>
      <th>Peso</th>
      <th>Edad</th>     
      <th>Rasa</th>
      <th>Color</th>
      <th>Precio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cans.each do |can| %>
      <tr>
        <td><%= can.peso %></td>
        <td><%= can.edad %></td>       
        <td><%= can.rasa %></td>
        <td><%= can.color %></td>
        <td><%= can.precio %></td>
        <td><%= link_to 'Show', can %></td>
        <td><%= link_to 'Edit', edit_can_path(can) %></td>
        <td><%= link_to 'Destroy', can, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'NUEVO CANINO', new_can_path %>
<%= link_to 'REPORTES', cans_reportes_path %>

